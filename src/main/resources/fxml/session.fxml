                                            <padding>
                                                <Insets left="10.0" right="10.0" top="5.0"/>
                                            </padding>
                                        </Label>
                                        <TextField fx:id="socketConnectHost" text="127.0.0.1"
                                                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                    </children>
                                </GridPane>
                            </children>
