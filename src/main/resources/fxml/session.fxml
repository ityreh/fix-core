                                    <children>
                                        <Label text="SocketConnectPort">
                                            <padding>
                                                <Insets left="10.0" right="10.0" top="5.0"/>
                                            </padding>
                                        </Label>
                                        <TextField fx:id="socketConnectPort" prefHeight="27.0" prefWidth="523.0"
                                                   text="9876" GridPane.columnIndex="1"/>
