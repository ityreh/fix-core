            <children>
                <Text fx:id="output" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Start your Session when you are ready with configuration." HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </HBox.margin>
                </Text>
            </children>
        </HBox>
        <HBox>
            <children>
                <Button fx:id="sessionButton" styleClass="view-button" mnemonicParsing="false" text="Start"
