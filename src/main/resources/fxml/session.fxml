                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    </rowConstraints>
                                    <children>
                                        <Label text="BeginString">
                                            <padding>
                                                <Insets left="10.0" right="10.0" top="5.0"/>
                                            </padding>
                                        </Label>
                                        <TextField fx:id="beginString" prefHeight="27.0" prefWidth="523.0"
                                                   text="FIX.4.2" GridPane.columnIndex="1"/>
                                        <Label text="SenderCompID" GridPane.rowIndex="1">
                                            <padding>
                                                <Insets left="10.0" right="10.0" top="5.0"/>
