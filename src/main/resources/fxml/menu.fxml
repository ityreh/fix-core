<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <HBox fx:id="sessionMenu" styleClass="btns">
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <children>
            <Label fx:id="sessionMenuLabel" text="Session" textFill="#b2aaaa">
                <font>
                    <Font size="18.0" />
                </font>
                <HBox.margin>
                    <Insets />
                </HBox.margin>
                <padding>
                    <Insets bottom="10.0" left="10.0" top="10.0" />
                </padding>
            </Label>
        </children>
    </HBox>
    <HBox fx:id="messageMenu" layoutX="10.0" layoutY="80.0" styleClass="btns">
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <children>
            <Label fx:id="messageMenuLabel" text="Message" textFill="#b2aaaa">
                <font>
                    <Font size="18.0" />
                </font>
                <padding>
                    <Insets bottom="10.0" left="10.0" top="10.0" />
                </padding>
            </Label>
        </children>
    </HBox>
    <HBox fx:id="logMenu" layoutX="10.0" layoutY="117.0" styleClass="btns">
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <children>
            <Label fx:id="logMenuLabel" text="Log" textFill="#b2aaaa">
                <font>
                    <Font size="18.0" />
                </font>
                <padding>
                    <Insets bottom="10.0" left="10.0" top="10.0" />
                </padding>
            </Label>
        </children>
    </HBox>
    <VBox prefHeight="200.0" prefWidth="100.0" styleClass="box" VBox.vgrow="ALWAYS">
        <children>
            <Label text="ENGINE" textFill="#6f6f6f">
                <font>
                    <Font size="14.0" />
                </font>
                <padding>
                    <Insets bottom="10.0" left="10.0" top="10.0" />
                </padding>
            </Label>
            <HBox>
                <children>
                    <Label text="O" textFill="#52ff34">
                        <font>
                            <Font size="10.0" />
                        </font>
                  <padding>
                     <Insets left="10.0" />
                  </padding>
                    </Label>
               <VBox prefHeight="200.0" prefWidth="100.0">
                  <children>
                     <Label text="FIXT.1.1" textFill="#a2bfa2">
                        <VBox.margin>
                           <Insets />
                        </VBox.margin>
                        <padding>
                           <Insets left="10.0" />
                        </padding></Label>
                     <Label text="Initiator" textFill="#a2bfa2">
                        <VBox.margin>
                           <Insets />
                        </VBox.margin>
                        <padding>
                           <Insets left="10.0" />
                        </padding></Label>
                     <Label text="Acceptor" textFill="#a2bfa2">
                        <padding>
                           <Insets left="10.0" />
                        </padding></Label>
                  </children>
               </VBox>
                </children>
            </HBox>
        </children>
    </VBox>
    <VBox>
        <children>
            <Label fx:id="version" text="Version: 0.0.1" textFill="WHITE">
                <padding>
                    <Insets bottom="10.0" left="10.0" top="10.0" />
                </padding>
            </Label>
        </children>
    </VBox>
</VBox>
